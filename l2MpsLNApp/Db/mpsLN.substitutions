#======================================
# 1-bit ENUMS with RBV
#======================================
file "Reg1BitRW.template" { pattern
{ R,                  DESC,                                     ZNAM,       ONAM,       SCAN,          PARAM                    }
{ ANA_FORCEPEAKEN,    "Enable Analog Force Peak Values",        "Disable",  "Enable",   "1 second",    ANACORE_ForcePeakValEn   }
}