#======================================
# Register with ScalVal interfaces
#======================================
file "RegWO.template" { pattern
{ R,                DESC,                                       PINI,   SCAN,       EGU,    PARAM               }
{ CON_USRRST,       "Counter Reset",                            "NO",   "Passive",  "",     CONCEN_UserReset    }
{ CON_CNTRST,       "Counter Reset",                            "NO",   "Passive",  "",     CONCEN_CountReset   }
}

